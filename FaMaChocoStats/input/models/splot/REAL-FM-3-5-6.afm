ban_flash.value: Integer[0 to 10],3,0;
ban_flash.cost: Integer[0 to 10],3,0;
keyword.value: Integer[0 to 10],3,0;
keyword.cost: Integer[0 to 10],8,0;
db.value: Integer[0 to 10],9,0;
db.cost: Integer[0 to 10],3,0;
file.value: Integer[0 to 10],5,0;
file.cost: Integer[0 to 10],8,0;
nttp.value: Integer[0 to 10],8,0;
nttp.cost: Integer[0 to 10],0,0;
