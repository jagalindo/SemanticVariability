%Constraints
keyword REQUIRES text;
dynamic REQUIRES active;
db REQUIRES database;
data_transfer REQUIRES https;
file REQUIRES ftp;
https EXCLUDES ms;
basic.cost != 4 IMPLIES user_auth.value > 7;
asp.cost >= 8 IMPLIES ban_img.cost < 9;
ban_img.value > 8 AND asp.cost != 4;
database.cost > 6 AND jsp.cost != 5;
xml.cost == 4 OR data_storage.cost <= 6;

