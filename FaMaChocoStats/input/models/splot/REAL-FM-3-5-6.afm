%Relationships
web_portal : [add_services]  web_server [persistence] [ri] [performance] ;
add_services : [site_stats] [site_search] [ad_server] ;
site_stats :  basic [advanced] ;
site_search : [images] [text] ;
text :  html [dynamic] ;
ad_server :  reports [popups]  banners [keyword] ;
banners :  ban_img [ban_flash] ;
web_server : [logging] [protocol]  cont ;
logging :  db  file ;
protocol : [1,3] {nttp ftp https };
cont :  static [active] ;
active : [1,4] {asp php jsp cgi };
persistence :  xml  database ;
ri : [1,3] {data_storage data_transfer user_auth };
performance :  ms  sec  mi_n ;

%Attributes
basic.value: Integer[0 to 10],10,0;
basic.cost: Integer[0 to 10],8,0;
